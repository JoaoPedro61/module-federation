{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"analytics": "dfefed9b-b1bd-4729-9ce5-45fb0f06d713",
		"packageManager": "yarn"
	},
	"newProjectRoot": "projects",
	"projects": {
		"auth": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				}
			},
			"root": "projects/auth",
			"sourceRoot": "projects/auth/src",
			"prefix": "ng",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/auth",
						"index": "projects/auth/src/index.html",
						"main": "projects/auth/src/main.ts",
						"polyfills": "projects/auth/src/polyfills.ts",
						"tsConfig": "projects/auth/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/auth/src/favicon.ico",
							"projects/auth/src/assets"
						],
						"styles": [
							"projects/auth/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/auth/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/auth/src/environments/environment.ts",
									"with": "projects/auth/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/auth/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "auth:build:production",
							"extraWebpackConfig": "projects/auth/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "auth:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/auth/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "auth:build",
						"extraWebpackConfig": "projects/auth/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/auth/src/test.ts",
						"polyfills": "projects/auth/src/polyfills.ts",
						"tsConfig": "projects/auth/tsconfig.spec.json",
						"karmaConfig": "projects/auth/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/auth/src/favicon.ico",
							"projects/auth/src/assets"
						],
						"styles": [
							"projects/auth/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
	},
	"defaultProject": "auth"
}